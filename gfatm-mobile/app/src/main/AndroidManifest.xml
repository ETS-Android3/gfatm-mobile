<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ihsinformatics.gfatmmobile"
    android:versionCode="1"
    android:versionName="0.0.1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/DarkTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:theme="@style/Theme.AppCompat.DayNight.Dialog.Alert"
            android:name=".ProgramActivity"
            android:label="@string/title_activity_program"></activity>
        <activity
            android:theme="@style/Theme.AppCompat.DayNight.Dialog.Alert"
            android:name=".ThemeActivity"
            android:label="@string/title_activity_theme"></activity>
        <activity
            android:theme="@style/Theme.AppCompat.DayNight.Dialog.Alert"
            android:name=".LanguageActivity"
            android:label="@string/title_activity_language"></activity>
        <activity
            android:theme="@style/Theme.AppCompat.DayNight.Dialog.Alert"
            android:name=".OperationModeActivity"
            android:label="@string/title_operation_mode"></activity>
        <activity
            android:theme="@style/Theme.AppCompat.DayNight.Dialog.Alert"
            android:name=".DefaultActivity"
            android:label="@string/title_activity_defaults"></activity>
        <activity
            android:theme="@style/Theme.AppCompat.DayNight.Dialog.Alert"
            android:name=".ServerActivity"
            android:label="@string/title_activity_server"></activity>
        <activity
            android:theme="@style/Theme.AppCompat.DayNight.Dialog.Alert"
            android:name=".SSLEncryptionActivity"
            android:label="@string/title_activity_ssl"></activity>
        <activity
            android:name=".SavedFormActivity"
            android:label="@string/title_saved_forms"></activity>
    </application>

</manifest>