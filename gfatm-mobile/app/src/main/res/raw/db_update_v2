-- Alter FORM table column p_id to nullable
CREATE TABLE FORM2(id INTEGER PRIMARY KEY AUTOINCREMENT,program TEXT NOT NULL COLLATE NOCASE,username TEXT NOT NULL COLLATE NOCASE,form_name TEXT NOT NULL COLLATE NOCASE,p_id TEXT COLLATE NOCASE,form_date DATETIME,timestamp DATETIME NOT NULL,location TEXT NOT NULL COLLATE NOCASE,form_object BLOB COLLATE NOCASE, encounter_id INT COLLATE NOCASE);
INSERT INTO FORM2 SELECT * FROM FORM;
DROP TABLE FORM;
ALTER TABLE FORM2 RENAME TO FORM;

