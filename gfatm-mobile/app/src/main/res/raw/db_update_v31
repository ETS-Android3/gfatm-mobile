-- create encounter type & encounter table
create table person_attribute_type(id integer primary key, name VARCHAR, description VARCHAR, format VARCHAR, uuid VARCHAR);
create table person_attribute(id integer primary key, patient_id integer, person_attribute_type_id integer, value VARCHAR, dateCreated datetime, voided boolean, uuid VARCHAR);